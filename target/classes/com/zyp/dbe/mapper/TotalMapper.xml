<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zyp.dbe.mapper.TotalMapper">

    <select id="getGradeByMajor" parameterType="String" resultType="com.zyp.dbe.pojo.Grades">
        select grades.* from student, grades where student.id = grades.studentID and student.major = #{major};
    </select>


</mapper>